{
  "name": "gatsby-plugin-breadcrumb",
  "version": "0.0.0-semantically-released",
  "description": "Gatsby plugin to add breadcrumbs to your site",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sbardian/gatsby-plugin-breadcrumb"
  },
  "homepage": "https://github.com/sbardian/gatsby-plugin-breadcrumb",
  "bugs": "https://github.com/sbardian/gatsby-plugin-breadcrumb/issues",
  "author": "Brian Andrews <sbardian@gmail.com>",
  "license": "MIT",
  "contributors": [
    "Brian Andrews <sbardian@gmail.com>"
  ],
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "breadcrumbs",
    "gatsby-plugin-breadcrumb"
  ],
  "scripts": {
    "cm": "git-cz",
    "build": "babel src --out-dir . --ignore **/__tests__",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "lint": "eslint ./src",
    "test": "jest",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "prepublishOnly": "yarn build"
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "babel-jest": "^25.0.0",
    "babel-preset-gatsby": "^0.3.0",
    "identity-obj-proxy": "^3.0.0",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@testing-library/dom": "7.2.1",
    "@testing-library/jest-dom": "5.5.0",
    "@testing-library/react": "10.0.3",
    "babel-eslint": "10.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-preset-gatsby-package": "0.3.1",
    "commitizen": "4.0.4",
    "coveralls": "3.0.12",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "gatsby": "2.20.28",
    "husky": "4.2.5",
    "jest": "25.4.0",
    "jest-dom": "4.0.0",
    "lint-staged": "10.1.7",
    "prettier": "2.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "semantic-release": "17.0.6"
  },
  "peerDependencies": {
    "gatsby": "^2.1.19"
  }
