{
  "name": "gatsby-plugin-breadcrumb",
  "version": "0.0.0-semantically-released",
  "description": "Gatsby plugin to add breadcrumbs to your site",
  "main": "index.js",
  "repository": "https://github.com/sbardian/gatsby-plugin-breadcrumb",
  "homepage": "https://github.com/sbardian/gatsby-plugin-breadcrumb",
  "bugs": "https://github.com/sbardian/gatsby-plugin-breadcrumb/issues",
  "author": "Brian Andrews <sbardian@gmail.com>",
  "license": "MIT",
  "contributors": [
    "Brian Andrews <sbardian@gmail.com>"
  ],
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "breadcrumbs",
    "gatsby-plugin-breadcrumb"
  ],
  "scripts": {
    "cm": "git-cz",
    "build": "babel src --out-dir . --ignore **/__tests__",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "lint": "eslint ./src",
    "test": "jest",
    "test:run": "cypress run",
    "test:open": "cypress open",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "prepublishOnly": "yarn build"
  },
  "release": {
    "branch": "master"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@testing-library/cypress": "4.2.0",
    "@testing-library/jest-dom": "4.1.2",
    "babel-eslint": "10.0.3",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-preset-gatsby-package": "0.2.9",
    "commitizen": "4.0.3",
    "cross-env": "6.0.3",
    "cypress": "3.4.1",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "gatsby": "2.16.4",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "react": "16.10.2",
    "semantic-release": "15.13.24"
  },
  "peerDependencies": {
    "gatsby": "^2.1.19"
  }
}
